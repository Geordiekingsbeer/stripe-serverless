{
  "builds": [
    {
      "src": "function/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/function/(.*)",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, GET, OPTIONS, PUT, PATCH, DELETE",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "status": 204
    },
    {
      "src": "/function/(.*)",
      "dest": "/function/$1"
    }
  ]
}
