{
  "builds": [
    {
      "src": "function/*.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/function/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,POST,PUT,PATCH,DELETE" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    }
  ]
}
