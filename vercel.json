{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*" // TEMPORARILY ALLOW ALL ORIGINS
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,DELETE,POST,PUT,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
