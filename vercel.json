{
  "builds": [
    {
      "src": "function/*.js",
      "use": "@vercel/node"
    }
  ],
  "headers": [
    {
      "source": "/function/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods": "GET, OPTIONS, POST" },
        { "key": "Access-Control-Allow-Headers": "Content-Type" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/function/(.*)",
      "dest": "/function/$1"
    }
  ]
}
